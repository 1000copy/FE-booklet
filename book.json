{
  "title": "FE-booklet",
  "description": "小火柴的前端小册子",
  "author": "xiaohuochai",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "小火柴的博客": "https://webhuochai.com"
    }
  },
  "styles":{
    "website":"style.css"
  },
  "variables": {
    "cdn": "://static.xiaohuochai.site/"
  },

  "plugins": [
    "-lunr",
    "-search",
    "-sharing",
    "-livereload",
    "expandable-chapters",
    "search-plus",
    "splitter",
    "github",
    "tbfed-pagefooter",
    "back-to-top-button"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/littlematch0123/FE-booklet"
    },
    "tbfed-pagefooter": {
      "copyright": "&copy; xiaohuochai",
      "modify_label": "文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  }
}